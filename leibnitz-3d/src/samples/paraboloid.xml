<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.mmarini.org/leibnitz-2-0-0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mmarini.org/leibnitz-2-0-0 ../../../leibnitz-lib/src/main/resources/leibnitz-2-0-0.xsd ">

	<function id="dt">1e-3</function>

	<function id="a">-0.5</function>
	<function id="g">9.81</function>

	<function id="q1_0">1</function>
	<function id="q2_0">0</function>

	<function id="qv1_0">0</function>
	<function id="qv2_0">sqrt(-2*a*g)*1.5</function>

	<function id="b0">(e0 + 0 * e1)</function>
	<function id="b1">e1</function>

	<variable id="q">q1_0 * e0 + q2_0 * e1</variable>
	<variable id="qv">qv1_0 * e0 + qv2_0 * e1</variable>

	<function id="q1">q * b0</function>
	<function id="q2">q * b1</function>

	<function id="qv1">qv * b0</function>
	<function id="qv2">qv * b1</function>

	<function id="qa2">-2 * qv1 * qv2 / q1</function>
	<function id="qa1">q1 * (2 * a * g - 4 * qv1^2 * a^2 + qv2^2) / (4 * q1^2 * a^2 + 1)</function>

	<function id="qa">qa1 * e0 + qa2 * e1</function>

	<function id="x">q1 * cos(q2)</function>
	<function id="z">-q1^2 * a</function>
	<function id="y">q1 * sin(q2)</function>
	<function id="r">x * e0 + y * e1 + z * e2</function>
	<function id="r0">0 * e2</function>

	<update id="qv">qv + qa * dt</update>
	<update id="q">q + qv * dt</update>

	<corpe location="r" />
	<corpe location="r0" />
</definitions>
