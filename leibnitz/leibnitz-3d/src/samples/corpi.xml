<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.mmarini.org/leibnitz-2-0-0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mmarini.org/leibnitz-2-0-0 ../../../leibnitz-lib/src/main/resources/leibnitz-2-0-0.xsd ">
	<function id="dt">10e-3</function>
	<variable id="t">0</variable>
	<function id="w1">2 * PI</function>
	<function id="a1">t * w1</function>
	<function id="xr1">e0 + 0 * e2</function>
	<function id="yr1">e1 + 0 * e2</function>
	<function id="A">xr1 * sin(a1) + yr1 * cos(a1)</function>

	<function id="w2">2 * PI / 10</function>
	<function id="a2">t * w2</function>
	<function id="xr2">2*e0 + 0 * e2</function>
	<function id="yr2">1 * e2</function>
	<function id="B">xr2 * sin(a2) + yr2 * cos(a2)</function>

	<function id="w3">2 * PI / 7</function>
	<function id="a3">t * w3</function>
	<function id="xr3">2*e1 + 0 * e2</function>
	<function id="yr3">1 * e2</function>
	<function id="C">xr3 * sin(a3) + yr3 * cos(a3)</function>

	<update id="t">t + dt</update>
	<corpe location="A" />
	<corpe location="B" />
	<corpe location="C" />
</definitions>
