<l:definitions xmlns:l="http://www.mmarini.org/leibnitz-1-0-0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mmarini.org/leibnitz-1-0-0 ../Leibnitz/res/leibnitz-1-0-0.xsd ">
	<l:variable id="q">1+0*e2</l:variable>
	<l:function id="dt">0.01</l:function>
	<l:function id="phi">90*PI/180</l:function>
	<l:function id="v">1*e0 + 1*e1 + 1*e2</l:function>
	<l:function id="r">0*e0 + 9*e1 + 0.3*e2</l:function>
	<l:function id="q1">qrot(phi*v*dt)</l:function>
	<l:update id="q">q*q1</l:update>
</l:definitions>
